{
  "name": "template-vite-react",
  "description": "A template for react projects built with vite",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "author": "Mike Valeriano <mkvlrn@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:mkvlrn/template-vite-react"
  },
  "keywords": [
    "vite",
    "react",
    "esm",
    "template"
  ],
  "engines": {
    "node": ">=24.14"
  },
  "imports": {
    "#/*": "./src/*.tsx"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production vite build",
    "dev": "vite --host",
    "start": "vite preview --host",
    "test": "vitest",
    "test-staged": "vitest related --bail=1 --reporter=github-actions",
    "test-ci": "vitest --bail=1 --reporter=github-actions",
    "biome-check": "biome check --diagnostic-level=warn --no-errors-on-unmatched",
    "biome-fix": "pnpm biome-check --write",
    "typecheck": "tsc-files vite-env.d.ts",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@mkvlrn/config": "^3.1.1",
    "@tailwindcss/postcss": "^4.2.1",
    "@tanstack/router-plugin": "^1.163.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "cross-env": "^10.1.0",
    "husky": "^9.1.7",
    "jsdom": "^28.1.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.2.1",
    "tsc-files": "^1.1.4",
    "typescript": "6.0.0-dev.20260225",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-router": "^1.163.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  }
}
