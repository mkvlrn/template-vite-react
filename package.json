{
  "name": "template-vite-react",
  "description": "A template for react projects built with vite",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "author": "Mike Valeriano <mkvlrn@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:mkvlrn/template-vite-react"
  },
  "keywords": [
    "vite",
    "react",
    "esm",
    "template"
  ],
  "engines": {
    "node": "24.x"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production vite build",
    "dev": "vite --host",
    "start": "vite preview --host",
    "test": "vitest",
    "biome-check": "biome check --no-errors-on-unmatched",
    "biome-fix": "yarn biome-check --write",
    "typecheck": "tsc",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@mkvlrn/config": "^2.0.19",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "cross-env": "^10.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.6",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "tsc-files": "^1.1.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
