{
  "name": "template-vite-react",
  "description": "A template for react projects built with vite",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "author": "Mike Valeriano <mkvlrn@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:mkvlrn/template-vite-react"
  },
  "keywords": [
    "vite",
    "react",
    "esm",
    "template"
  ],
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production vite build",
    "dev": "vite --host",
    "start": "vite preview --host",
    "test": "vitest",
    "biome-check": "biome check --no-errors-on-unmatched",
    "biome-fix": "pnpm biome-check --write",
    "typecheck": "tsc",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@mkvlrn/config": "^2.0.24",
    "@tailwindcss/postcss": "^4.1.14",
    "@tanstack/router-plugin": "^1.132.41",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "cross-env": "^10.1.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "tsc-files": "^1.1.4",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-router": "^1.132.41",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}
