{
  "name": "template-vite-react",
  "description": "A template for react projects built with vite",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "author": "Mike Valeriano <mkvlrn@proton.me>",
  "repository": {
    "type": "git",
    "url": "git@github.com:mkvlrn/template-vite-react"
  },
  "keywords": [
    "vite",
    "react",
    "esm",
    "template"
  ],
  "engines": {
    "node": ">=23.8"
  },
  "scripts": {
    "build": "NODE_ENV=production npx vite build",
    "dev": "vite --host",
    "start": "vite preview --host",
    "test": "vitest",
    "biome-check": "biome check --no-errors-on-unmatched",
    "biome-fix": "yarn biome-check --write",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.0-beta.3",
    "@commitlint/cli": "^19.8.1",
    "@mkvlrn/config": "^2.0.6",
    "@tailwindcss/postcss": "^4.1.6",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.4",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.6",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
